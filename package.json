    "prepare": "husky install"
  },
  "dependencies": {
    "@next/env": "^13.0.4",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "date-fns": "^2.29.3",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3",
    "usehooks-ts": "^2.9.1"
  },
  "pnpm": {
    "overrides": {
      "enhanced-resolve": "5.10.0"
    }
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.0-alpha.52",
    "@storybook/addon-essentials": "^7.0.0-alpha.52",
