    "eslint-config-prettier": "^8.5.0",
    "dotenv": "^16.0.3",
    "eslint-plugin-storybook": "^0.6.7",
    "husky": ">=7",
    "lint-staged": ">=10",
    "node-fetch": "^3.3.0",
    "openapi-typescript": "^6.0.3",
    "openapi-typescript-fetch": "^1.1.3",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.8.0",
    "storybook": "^7.0.0-alpha.52",
    "style-loader": "^3.3.1",
    "tsx": "^3.12.1"
